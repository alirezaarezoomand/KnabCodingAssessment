{
  "CoinMarketCapOptions": {
    "ApiKey": "5b7b8d97-329e-4a35-8c0c-44a01892fcc5",
    "ApiUrl": "https://pro-api.coinmarketcap.com/v2/"
  },
  "QuoteCurrencies": [ "USD", "EUR", "BRL", "GBP", "AUD" ],
  "ApiConfiguration": {
    "ApiVersions": [
      "v1.0"
    ],
    "ApiDefaultVersion": "v1.0",
    "ApiSupportedLanguages": [
      "fa",
      "en"
    ],
    "ApiDefaultLanguage": "en",
    "ApiName": "Exchange API",
    "ApiBaseUrl": "http://localhost:30000",
    "IsSwaggerEnabled": true,
    "RequireHttpsMetadata": false,
    "CorsAllowAnyOrigin": true,
    "CorsAllowOrigins": [ "" ],
    "TimeoutInMilliseconds": 30000
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*"
}
